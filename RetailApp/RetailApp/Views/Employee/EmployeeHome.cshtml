@model IEnumerable<RetailApp.Models.Schedule>

@{
    ViewBag.Title = "EmployeeHome";
}

<h2>Employee Home</h2>
<br />
<div class="panel panel-info">
    <div class="panel-heading">
        <h3 class="panel-title text-center">Today's Date</h3>
    </div>
    <div class="panel-body text-center">
        <strong>@System.DateTime.Now</strong>
    </div>
</div>
 <br /> 
<div>
    <table class="table table-hover table-responsive table-striped table-striped table-bordered">
        <tr class="info">
            <th>
                @Html.DisplayNameFor(model => model.StartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndTime)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.StartTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EndTime)
                </td>
            </tr>
        }

    </table>
</div>

